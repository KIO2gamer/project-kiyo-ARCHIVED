{
	"name": "project-kiyo",
	"version": "1.0.0",
	"description": "Kiyo Discord Bot with MERN Dashboard",
	"engines": {
		"node": ">=18.0.0"
	},
	"main": "server.js",
	"scripts": {
		"start": "node server.js",
		"server": "nodemon server.js",
		"client": "npm start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"build": "npm install && cd client && npm install && npm run build",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KIO2gamer/discordbot.git"
	},
	"homepage": "https://kio2gamer.github.io/project-kiyo/",
	"keywords": [
		"discord",
		"bot",
		"dashboard",
		"mern"
	],
	"author": "KIO2gamer",
	"license": "MIT",
	"dependencies": {
		"axios": "^1.3.4",
		"concurrently": "^8.0.1",
		"cors": "^2.8.5",
		"discord.js": "^14.7.1",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"he": "^1.2.0",
		"jsonwebtoken": "^9.0.0",
		"mongoose": "^7.0.3",
		"node-cache": "^5.1.2"
	},
	"devDependencies": {
		"nodemon": "^2.0.22"
	},
	"bugs": {
		"url": "https://github.com/KIO2gamer/discordbot/issues"
	},
	"funding": {
		"type": "github",
		"url": "https://github.com/sponsors/KIO2gamer"
	},
	"directories": {
		"doc": "docs"
	}
}
