{
	"name": "kiyo-discord-bot",
	"version": "1.1.0",
	"description": "A versatile Discord bot with moderation, entertainment, and utility features.",
	"engines": {
		"node": ">=16.9.0",
		"npm": ">=7.0.0"
	},
	"main": "src/index.js",
	"scripts": {
		"start": "node src/index.js",
		"dev": "nodemon src/index.js",
		"dashboard": "node src/dashboard/index.js",
		"dev:dashboard": "nodemon src/dashboard/index.js",
		"build": "npm run build:dashboard && npm run copy-assets",
		"build:dashboard": "mkdir -p dist && cp -r src/dashboard/public dist/ && cp -r src/dashboard/views dist/views && cp -r src/dashboard/routes dist/routes && cp src/dashboard/index.js dist/",
		"copy-assets": "cp -r src/dashboard/public/* dist/",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"format": "prettier --config .prettierrc.json --write .",
		"format:check": "prettier --config .prettierrc.json --check ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KIO2gamer/discordbot.git"
	},
	"homepage": "https://kio2gamer.github.io/project-kiyo/",
	"keywords": [
		"discord",
		"bot",
		"moderation",
		"fun",
		"games",
		"utility",
		"api",
		"discord.js"
	],
	"author": "KIO2gamer (https://kio2gamer.carrd.co/)",
	"contributors": [
		{
			"name": "Neon",
			"email": "neon@saahild.com",
			"url": "https://saahild.com"
		}
	],
	"license": "MIT",
	"nodemonConfig": {
		"ignore": [
			"assets/",
			"docs/",
			"coverage/",
			"dist/"
		],
		"ext": "js,json",
		"watch": [
			"src/"
		]
	},
	"dependencies": {
		"@discord/embedded-app-sdk": "^1.8.1",
		"@google/generative-ai": "^0.21.0",
		"@iamtraction/google-translate": "^2.0.1",
		"axios": "^1.7.8",
		"chalk": "^4.1.2",
		"common-tags": "^1.8.2",
		"connect-mongo": "^5.1.0",
		"discord.js": "^14.17.2",
		"dotenv": "^16.4.5",
		"ejs": "^3.1.10",
		"express": "^4.18.2",
		"express-ejs-layouts": "^2.5.1",
		"express-rate-limit": "^7.5.0",
		"express-session": "^1.18.1",
		"googleapis": "^144.0.0",
		"he": "^1.2.0",
		"helmet": "^8.0.0",
		"mathjs": "^14.0.0",
		"moment": "^2.30.1",
		"mongoose": "^8.8.3",
		"morgan": "^1.10.0",
		"ms": "^2.1.3",
		"node-cache": "^5.1.2",
		"node-fetch": "^2.7.0",
		"passport": "^0.7.0",
		"passport-discord": "^0.1.4",
		"serverless-http": "^3.2.0",
		"winston": "^3.17.0"
	},
	"devDependencies": {
		"eslint": "^9.15.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"globals": "^15.12.0",
		"husky": "^8.0.3",
		"jsdoc": "^4.0.2",
		"lint-staged": "^15.0.2",
		"nodemon": "^3.1.9",
		"prettier": "^3.4.1",
		"rimraf": "^5.0.1"
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,md}": [
			"prettier --write"
		]
	},
	"bugs": {
		"url": "https://github.com/KIO2gamer/discordbot/issues"
	},
	"funding": {
		"type": "github",
		"url": "https://github.com/sponsors/KIO2gamer"
	},
	"directories": {
		"doc": "docs"
	}
}
